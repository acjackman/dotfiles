# -*- mode: zsh -*- vim: ft=zsh

remote_url=$(git remote get-url origin)

if [[ "$remote_url" =~ github\.com ]]; then
  gh pr view --json=title,number,url,headRepository -t ":pr: {{.title}}: [{{.headRepository.name}}#{{.number}}]({{.url}})" | pbcopy
else;
  >&2 echo unsupported origin $remote_url
fi
