{
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "flake8helled",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.formatting.provider": "black",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono','FiraCode Nerd Font Mono','Hack Nerd font Mono','JetBrains Mono', 'Fira Code', 'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.allowChords": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.autoIndent": "brackets",
  "editor.inlineSuggest.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.rulers": [88],
  "explorer.autoReveal": false,
  "redhat.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.defaultTimeFormat": "HH:mm",
  "python.sortImports.path": "isort",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.analysis.completeFunctionParens": true,
  "cSpell.userWords": [
    "acjackman",
    "ajackman",
    "boto",
    "botocore",
    "cname",
    "contentful",
    "creds",
    "delenv",
    "envvar",
    "fasr",
    "fl",
    "fulcrumlabs",
    "gitlab",
    "infodns",
    "ipython",
    "isclass",
    "isfunction",
    "isort",
    "itertools",
    "kubernetes",
    "moto",
    "mrowens",
    "nobreak",
    "noqa",
    "pardot",
    "pyenv",
    "pytest",
    "setenv",
    "Traceback",
    "unallowed",
    "venv",
    "virtualenv"
  ],
  "scm.countBadge": "focused",
  "scm.inputFontSize": 15,
  "markdown.preview.fontSize": 16,
  "markdown.preview.lineHeight": 1.4,
  "[python]": {
    "gitlens.codeLens.symbolScopes": ["!Module"],
    // "editor.codeActionsOnSave": {
    //     "source.organizeImports": true
    // },
    "editor.wordBasedSuggestions": false
  },
  "editor.formatOnSave": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "editor.minimap.maxColumn": 100,
  "workbench.editor.highlightModifiedTabs": true,
  "git.confirmSync": false,
  "workspaceExplorer.workspaceStorageDirectory": "${env:HOME}/Development/",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Atom One Dark",
  "security.workspace.trust.banner": "always",
  "liveshare.connectionMode": "relay",
  "security.workspace.trust.untrustedFiles": "open",
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "editor.accessibilitySupport": "off",
  "workbench.editor.showTabs": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "peacock.showColorInStatusBar": false,
  "vim.statusBarColorControl": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#5f0000",
    "statusBar.noFolderBackground": "#5f0000",
    "statusBar.debuggingBackground": "#5f0000",
    "activityBar.activeBackground": "#ff6433",
    "activityBar.activeBorder": "#00ff3d",
    "activityBar.background": "#ff6433",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#00ff3d",
    "activityBarBadge.foreground": "#15202b",
    "titleBar.activeBackground": "#ff3d00",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#ff3d0099",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#2f7c47",
    "editorGroup.border": "#2f7c47",
    "panel.border": "#2f7c47",
    "sash.hoverBorder": "#ff6433",
    "sideBar.border": "#2f7c47"
  },
  "peacock.affectStatusBar": false,
  "terminal.integrated.cursorStyle": "line",
  "macros": {
    "execCurLn": [
      "expandLineSelection",
      "workbench.action.terminal.runSelectedText",
      "cancelSelection"
    ]
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.visible": false,
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "addWords": true,
      "scope": "user"
    }
  },
  "python.testing.pytestEnabled": true,
  "python.terminal.activateEnvironment": false,
  "settingsSync.ignoredSettings": [
    "-python.linting.flake8Path"
  ]
}