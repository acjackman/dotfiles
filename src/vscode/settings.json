{
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.wordBasedSuggestions": false,
    "gitlens.codeLens.symbolScopes": ["!Module"]
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.customDictionaries": {
    "custom-dictionary-user": {
      "addWords": true,
      "name": "custom-dictionary-user",
      "path": "~/.cspell/custom-dictionary-user.txt",
      "scope": "user"
    }
  },
  "cSpell.userWords": [
    "acjackman",
    "ajackman",
    "boto",
    "botocore",
    "cname",
    "contentful",
    "creds",
    "delenv",
    "envvar",
    "fasr",
    "fl",
    "fulcrumlabs",
    "gitlab",
    "infodns",
    "ipython",
    "isclass",
    "isfunction",
    "isort",
    "itertools",
    "kubernetes",
    "moto",
    "mrowens",
    "nobreak",
    "noqa",
    "pardot",
    "pyenv",
    "pytest",
    "pytrace",
    "setenv",
    "Traceback",
    "unallowed",
    "venv",
    "virtualenv"
  ],
  "editor.accessibilitySupport": "off",
  "editor.autoIndent": "brackets",
  "editor.bracketPairColorization.enabled": true,
  "editor.defaultFormatter": null,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono','FiraCode Nerd Font Mono','Hack Nerd font Mono','JetBrains Mono', 'Fira Code', 'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 100,
  "editor.rulers": [88],
  "editor.suggest.localityBonus": true,
  "editor. stickyScroll.enabled": true,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "git.confirmSync": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.defaultTimeFormat": "HH:mm",
  "liveshare.connectionMode": "relay",
  "macros": {
    "execCurLn": [
      "expandLineSelection",
      "workbench.action.terminal.runSelectedText",
      "cancelSelection"
    ]
  },
  "markdown.preview.fontSize": 16,
  "markdown.preview.lineHeight": 1.4,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "peacock.affectEditorGroupBorder": true,
  "peacock.affectPanelBorder": true,
  "peacock.affectSideBarBorder": true,
  "peacock.affectStatusAndTitleBorders": true,
  "peacock.showColorInStatusBar": true,
  "peacock.vslsJoinColor": "#215732",
  "python.analysis.completeFunctionParens": true,
  "python.formatting.provider": "black",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/Users/acjackman/.pyenv/shims/flake8helled",
  "python.linting.lintOnSave": true,
  "python.linting.pylintEnabled": false,
  "python.sortImports.path": "isort",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.terminal.activateEnvironment": false,
  "redhat.telemetry.enabled": false,
  "scm.countBadge": "focused",
  "scm.inputFontSize": 15,
  "security.workspace.trust.banner": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "settingsSync.ignoredSettings": ["-python.linting.flake8Path"],
  "terminal.integrated.allowChords": false,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.scrollback": 10000,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "vim.camelCaseMotion.enable": true,
  "vim.history": 500,
  "vim.overrideCopy": true,
  "vim.sneak": true,
  "vim.statusBarColorControl": false,
  "vim.useSystemClipboard": true,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ab307e",
    "activityBar.activeBorder": "#25320e",
    "activityBar.background": "#ab307e",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#25320e",
    "activityBarBadge.foreground": "#e7e7e7",
    "editorGroup.border": "#2f7c47",
    "panel.border": "#2f7c47",
    "sash.hoverBorder": "#ab307e",
    "sideBar.border": "#2f7c47",
    "statusBar.background": "#5f0000",
    "statusBar.debuggingBackground": "#5f0000",
    "statusBar.foreground": "#e7e7e7",
    "statusBar.noFolderBackground": "#5f0000",
    "statusBarItem.hoverBackground": "#2f7c47",
    "titleBar.activeBackground": "#832561",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#83256199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "workbench.colorTheme": "Atom One Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.startupEditor": "none",
  "workspaceExplorer.workspaceStorageDirectory": "${env:HOME}/Development/",
  "files.autoSave": "onFocusChange"
}
