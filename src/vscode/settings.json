{
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "flake8helled",
  "python.linting.enabled": true,
  "python.linting.lintOnSave": true,
  "python.formatting.provider": "black",
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono','FiraCode Nerd Font Mono','Hack Nerd font Mono','JetBrains Mono', 'Fira Code', 'Hack', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.fontLigatures": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.minimap.enabled": false,
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.scrollback": 10000,
  "peacock.vslsShareColor": "#215732",
  "terminal.integrated.allowChords": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.autoIndent": "brackets",
  "editor.inlineSuggest.enabled": true,
  "editor.suggest.localityBonus": true,
  "editor.rulers": [
    88,
  ],
  "redhat.telemetry.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm",
  "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
  "gitlens.defaultTimeFormat": "HH:mm",
  "python.sortImports.path": "isort",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "python.analysis.completeFunctionParens": true,
  "cSpell.userWords": [
    "ajackman",
    "boto",
    "botocore",
    "cname",
    "contentful",
    "creds",
    "delenv",
    "fasr",
    "fl",
    "fulcrumlabs",
    "gitlab",
    "infodns",
    "ipython",
    "isclass",
    "isfunction",
    "itertools",
    "kubernetes",
    "moto",
    "mrowens",
    "nobreak",
    "noqa",
    "pardot",
    "pytest",
    "setenv",
    "unallowed"
  ],
  "scm.countBadge": "focused",
  "scm.inputFontSize": 15,
  "markdown.preview.fontSize": 16,
  "markdown.preview.lineHeight": 1.4,
  "[python]": {
    "gitlens.codeLens.symbolScopes": [
      "!Module"
    ],
    // "editor.codeActionsOnSave": {
    //     "source.organizeImports": true
    // },
    "editor.wordBasedSuggestions": false
  },
  "editor.formatOnSave": true,
  "editor.minimap.maxColumn": 100,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.showTabs": false,
  "git.confirmSync": false,
  "workspaceExplorer.workspaceStorageDirectory": "${env:HOME}/Development/",
  "peacock.vslsJoinColor": "#215732",
  "workbench.preferredDarkColorTheme": "Visual Studio Dark",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Atom One Dark",
  "security.workspace.trust.banner": "always",
  "liveshare.connectionMode": "relay",
  "security.workspace.trust.untrustedFiles": "open",
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false
}
